<!DOCTYPE html>
<!--[if lt IE 7]>
<html class="no-js sidebar-large lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>
<html class="no-js sidebar-large lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>
<html class="no-js sidebar-large lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!-->
<html class="no-js sidebar-large"> <!--<![endif]-->

<head>
    <!-- BEGIN META SECTION -->
    <meta charset="utf-8">
    <title>{% block title %}Admin{% endblock %}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta content="" name="description"/>
    <meta content="themes-lab" name="author"/>
    <link rel="shortcut icon" href="{{ asset('admin/assets/img/favicon.png') }}">
    <!-- END META SECTION -->
    <!-- BEGIN MANDATORY STYLE -->

    <link href="{{ asset('admin/assets/css/icons/icons.min.css') }}" rel="stylesheet">
    <link href="{{ asset('admin/assets/css/bootstrap.min.css') }}" rel="stylesheet">
    <link href="{{ asset('admin/assets/css/plugins.min.css') }}" rel="stylesheet">
    <link href="{{ asset('admin/assets/css/style.min.css') }}" rel="stylesheet">
    <link href="#" rel="stylesheet" id="theme-color">
    <!-- END  MANDATORY STYLE -->
    <!-- BEGIN PAGE LEVEL STYLE -->
    <link rel="stylesheet" href="{{ asset('admin/assets/plugins/datatables/dataTables.css') }}">
    <!-- END PAGE LEVEL STYLE -->
    <script src="{{ asset('admin/assets/plugins/modernizr/modernizr-2.6.2-respond-1.1.0.min.js') }}"></script>
    {% block stylesheets %}
    {% endblock %}
</head>

<body data-page="tables_editable" ng-app="MyApp">
{% include 'admin.header.twig' %}
<!-- BEGIN WRAPPER -->
<div id="wrapper">
    {% include 'admin.sidebar.twig' %}
    <!-- BEGIN MAIN CONTENT -->
    <div id="main-content">
        <div class="page-title"><i class="icon-custom-left"></i>
            <h3>{% block panelheading %}Admin{% endblock %}</h3>
        </div>
        <div class="row">
            <div class="col-md-12">
                {% block content %}{% endblock %}
            </div>
        </div>
    </div>
    <!-- END MAIN CONTENT -->
</div>
<!-- END WRAPPER -->
{% if false %}
    {% include 'layouts/admin.chatbar.twig' %}
{% endif %}
<!-- BEGIN MANDATORY SCRIPTS -->
<script src="{{ asset('admin/assets/plugins/jquery-1.11.js') }}"></script>
<script src="{{ asset('admin/assets/plugins/jquery-migrate-1.2.1.js') }}"></script>
<script src="{{ asset('admin/assets/plugins/jquery-ui/jquery-ui-1.10.4.min.js') }}"></script>
<script src="{{ asset('admin/assets/plugins/jquery-mobile/jquery.mobile-1.4.2.js') }}"></script>
<script src="{{ asset('admin/assets/plugins/bootstrap/bootstrap.min.js') }}"></script>
<script src="{{ asset('admin/assets/plugins/bootstrap-dropdown/bootstrap-hover-dropdown.min.js') }}"></script>
{#<script src="{{ asset('admin/assets/plugins/bootstrap-select/bootstrap-select.js') }}"></script>#}
<script src="{{ asset('admin/assets/plugins/mcustom-scrollbar/jquery.mCustomScrollbar.concat.min.js') }}"></script>
<script src="{{ asset('admin/assets/plugins/mmenu/js/jquery.mmenu.min.all.js') }}"></script>
<script src="{{ asset('admin/assets/plugins/nprogress/nprogress.js') }}"></script>
<script src="{{ asset('admin/assets/plugins/charts-sparkline/sparkline.min.js') }}"></script>
<script src="{{ asset('admin/assets/plugins/breakpoints/breakpoints.js') }}"></script>
<script src="{{ asset('admin/assets/plugins/numerator/jquery-numerator.js') }}"></script>
<script src="{{ asset('admin/assets/plugins/jquery.cookie.min.js') }}" type="text/javascript"></script>
<!-- END MANDATORY SCRIPTS -->
<!-- BEGIN PAGE LEVEL SCRIPTS -->
<script src="{{ asset('admin/assets/plugins/datatables/dynamic/jquery.dataTables.min.js') }}"></script>
<script src="{{ asset('admin/assets/plugins/datatables/dataTables.bootstrap.js') }}"></script>
<script src="{{ asset('admin/assets/plugins/datatables/dataTables.tableTools.js') }}"></script>
<script src="{{ asset('admin/assets/js/table_editable.js') }}"></script>
<!-- END  PAGE LEVEL SCRIPTS -->

{% block javascripts %}
{% endblock %}
<script src="{{ asset('admin/assets/js/application.js') }}"></script>
</body>

</html>
