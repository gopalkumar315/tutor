<!-- Menu Toggle Script -->
<script>
    $("#menu-toggle").click(function(e) {
        e.preventDefault();
        $("#wrapper").toggleClass("toggled");
    });
</script>

<style type="text/css">
    #page-content-wrapper{
        display:none;
    }
    @media screen and (max-width:800px) {
